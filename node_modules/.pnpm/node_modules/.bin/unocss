#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tiandaoyi/docker-project/study/node_modules/.pnpm/registry.npmmirror.com+@unocss+cli@0.51.12/node_modules/@unocss/cli/bin/node_modules:/Users/tiandaoyi/docker-project/study/node_modules/.pnpm/registry.npmmirror.com+@unocss+cli@0.51.12/node_modules/@unocss/cli/node_modules:/Users/tiandaoyi/docker-project/study/node_modules/.pnpm/registry.npmmirror.com+@unocss+cli@0.51.12/node_modules/@unocss/node_modules:/Users/tiandaoyi/docker-project/study/node_modules/.pnpm/registry.npmmirror.com+@unocss+cli@0.51.12/node_modules:/Users/tiandaoyi/docker-project/study/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tiandaoyi/docker-project/study/node_modules/.pnpm/registry.npmmirror.com+@unocss+cli@0.51.12/node_modules/@unocss/cli/bin/node_modules:/Users/tiandaoyi/docker-project/study/node_modules/.pnpm/registry.npmmirror.com+@unocss+cli@0.51.12/node_modules/@unocss/cli/node_modules:/Users/tiandaoyi/docker-project/study/node_modules/.pnpm/registry.npmmirror.com+@unocss+cli@0.51.12/node_modules/@unocss/node_modules:/Users/tiandaoyi/docker-project/study/node_modules/.pnpm/registry.npmmirror.com+@unocss+cli@0.51.12/node_modules:/Users/tiandaoyi/docker-project/study/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@unocss/cli/bin/unocss.mjs" "$@"
else
  exec node  "$basedir/../@unocss/cli/bin/unocss.mjs" "$@"
fi
